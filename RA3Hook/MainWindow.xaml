<Window x:Class="RA3Hook.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RA3Hook"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="image" Margin="0,0,0,0" StretchDirection="DownOnly" Stretch="None" Source="/AI-Generated-Imagejpg.jpg"/>
        <Button x:Name="button" Content="开始游戏" Click="OnStartGameButtonClicked" HorizontalAlignment="Left" Height="65" Margin="300,315,0,0" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="150,285,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="505" Height="27"/>
    </Grid>
</Window>
